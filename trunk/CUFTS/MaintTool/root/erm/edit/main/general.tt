      <div class="data">
       <select name="erm-edit-input-content-types" id="erm-edit-input-content-types" multiple="multiple">
        [% FOREACH content_type IN content_type_options %]
         [% SET content_type_id = content_type.id %]
        <option value="[% content_type_id %]" [% 'selected="selected"' IF active_content_types.$content_type_id %]>[% content_type.content_type | html %]</option>
        [% END %]
